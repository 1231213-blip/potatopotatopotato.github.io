-- Define a table of potatoes
local potatoes = {
    {type = "Russet", color = "Brown", weight = 300},
    {type = "Yukon Gold", color = "Yellow", weight = 250},
    {type = "Red Bliss", color = "Red", weight = 200},
    {type = "Fingerling", color = "Purple", weight = 150}
}

-- Function to print potato details
local function printPotato(potato)
    print("Type: " .. potato.type .. ", Color: " .. potato.color .. ", Weight: " .. potato.weight .. "g")
end

-- Iterate and print details of each potato
for _, potato in ipairs(potatoes) do
    printPotato(potato)
end
